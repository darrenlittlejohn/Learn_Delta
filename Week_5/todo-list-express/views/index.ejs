<h2>My Todo Lists </h2>

<ul>
  <% for(let index = 0; index < todoLists.length; index++){ %>
    <% let todoList = todoLists[index] %>
    <li>
      <a href="/todo-list/<%= todoList.get('id') %>">
        <%= todoList.get('name') %>
      </a>
    </li>
  <% } %>
</ul>

<div>
  <form
    action="/todo-list/new"
    method="post"
  >
    <input type='text' name='name' />
    <input type='submit' value='Create Todo List' />
  </form>
</div>
